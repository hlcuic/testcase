<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.hlcui.dao.Datamapper">

	<select id="findById" resultType="com.hlcui.model.TraderVo" parameterType="int">
		select * from test.traderinfo where id = #{id}
	</select>
	
	<insert id="insert" parameterType="com.hlcui.model.TraderVo">
		insert into test.traderinfo(name)values(#{name})
	</insert>
	
	<insert id="batchInsert" parameterType="java.util.List">
		insert into test.traderinfo(name)values
		 <foreach collection="list" index="index" item="item" separator=",">
               ( #{item.name} )      
        </foreach>
	</insert>
	
</mapper>